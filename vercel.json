{
  "version": 2,
  "name": "portfolio",
  "builds": [
    {
      "src": "portfolio/client/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "portfolio/client/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "regions": ["sfo1"]
}
